<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21"
            fx:controller="org.itdhbw.futurewars.application.controllers.ui.OptionsViewController">
    <SplitPane dividerPositions="0.07190635451505016" layoutX="152.0" layoutY="59.0" prefHeight="200.0"
               prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
        <Button fx:id="backButton" mnemonicParsing="false" onAction="#goBack" text="Back"/>
        <VBox prefHeight="200.0" prefWidth="100.0">
            <MenuButton fx:id="resolutionButton" mnemonicParsing="false" text="Resolution">
                <items>
                    <MenuItem mnemonicParsing="false" text="2560x1440"/>
                    <MenuItem mnemonicParsing="false" text="1920x1080"/>
                    <MenuItem mnemonicParsing="false" text="1600x900"/>
                    <MenuItem mnemonicParsing="false" text="1366x768"/>
                    <MenuItem mnemonicParsing="false" text="1280x720"/>
                    <MenuItem mnemonicParsing="false" text="1024x768"/>
                    <MenuItem mnemonicParsing="false" text="800x600"/>
                </items>
            </MenuButton>
            <MenuButton fx:id="viewModeButton" mnemonicParsing="false" text="ViewMode">
                <items>
                    <MenuItem mnemonicParsing="false" text="Fullscreen"/>
                    <MenuItem disable="true" mnemonicParsing="false" text="Borderless"/>
                    <MenuItem mnemonicParsing="false" text="Windowed"/>
                </items>
            </MenuButton>
        </VBox>
    </SplitPane>
</AnchorPane>
